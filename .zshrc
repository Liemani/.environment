# local variable
programmingfolderpath="~/programming"
gitfolderpath="$programmingfolderpath/git"
localfolderpath="$programmingfolderpath/local"

# source
alias sourcezshrc="source ~/.zshrc"

# cd
alias cdgit="cd $gitfolderpath"
alias cdlocal="cd $localfolderpath"

# vim
alias vimzshrc="vim ~/.zshrc"
alias vimvimrc="vim ~/.vimrc"
alias vimmemo="vim $gitfolderpath/memo/memo.txt"

